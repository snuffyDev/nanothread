{
	"name": "nanothreads",
	"author": "snuffydev",
	"repository": {
		"url": "https://github.com/snuffyDev/nanothreads"
	},
	"license": "ISC",
	"version": "0.2.21",
	"source": "./src/index.ts",
	"description": "a tiny cross-platform Worker & concurrency library",
	"keywords": [
		"threads",
		"worker",
		"concurrency",
		"async",
		"threading",
		"browser",
		"mutex"
	],
	"files": [
		"dist",
		"package.json"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"scripts": {
		"build": "tsup-node",
		"build:web": "tsup-node --config tsup.config.web.ts",
		"build:demo": "tsc --moduleResolution node --target ESNext -m  esnext ./index.ts",
		"start": "pnpm run build && node --loader=ts-node/esm --experimental-specifier-resolution=node ./index",
		"format": "prettier --write .",
		"test": "size-limit"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"browser": "./dist/index.js",
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./sync": {
			"import": "./dist/sync/index.js",
			"require": "./dist/sync/index.cjs",
			"browser": "./dist/sync/index.js",
			"default": "./dist/sync/index.js",
			"types": "./dist/sync/index.d.ts"
		},
		"./internals": {
			"import": "./dist/internals/index.js",
			"require": "./dist/internals/index.cjs",
			"default": "./dist/internals/index.cs",
			"types": "./dist/internals/index.d.ts"
		},
		"./models": {
			"types": "./dist/models/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"size-limit": [
		{
			"limit": "1 KB",
			"import": "*",
			"path": "src/index.ts"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.1.0",
		"@types/node": "^18.8.3",
		"size-limit": "^8.1.0",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"prettier": "^2.8.0",
		"ts-node": "^10.9.1",
		"tsup": "^6.5.0"
	}
}
